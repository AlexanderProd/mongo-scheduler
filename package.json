{
  "name": "mongo-scheduler-more",
  "description": "Persistent event scheduler using mongo as storage",
  "version": "1.2.2",
  "keywords": [
    "job",
    "jobs",
    "scheduler",
    "schedule",
    "event",
    "mongo",
    "cron",
    "more"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/darkterra/mongo-scheduler.git"
  },
  "dependencies": {
    "cron-parser": "^2.8.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongodb": "^3.1.13"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "codacy-coverage": "^3.4.0",
    "karma": "^4.0.1",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "projectz": "^1.7.4",
    "should": "^13.2.3",
    "sinon": "^7.2.7"
  },
  "scripts": {
    "test": "projectz compile; mocha -R nyan test/ --exit",
    "test:coverage": "projectz compile; nyc --reporter=lcov mocha -R nyan test/ --exit",
    "test:all": "npm run test:coverage; cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage"
  },
  "license": "MIT",
  "engine": {
    "node": ">= 6.0.0"
  },
  "author": "Jérémy Young <jeremy.young.pro@gmail.com>",
  "contributors": [
    "Jan Paul Erkelens (https://github.com/jperkelens)",
    "Matt Roman (https://github.com/romanmt)",
    "Jérémy Young (http://darkterra.fr)",
    "Codacy Badger (http://www.codacy.com)"
  ],
  "jshintConfig": {
    "eqeqeq": true,
    "indent": 2,
    "esnext": true,
    "asi": true,
    "maxlen": 80,
    "singleGroups": true,
    "unused": true
  },
  "maintainers": [],
  "bugs": {
    "url": "https://github.com/darkterra/mongo-scheduler/issues"
  }
}
